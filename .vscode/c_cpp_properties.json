{
    "version": 4,
    "configurations": [
        {
            "name": "Arduino",
            "includePath": [
                "${workspaceFolder}/**",
                "${HOME}/.arduino15/packages/arduino/hardware/avr/*/cores/arduino",
                "${HOME}/.arduino15/packages/arduino/hardware/avr/*/variants/eightanaloginputs",
                "${HOME}/.arduino15/packages/arduino/hardware/avr/*/libraries/**"
            ],
            "defines": [
                "ARDUINO=10819",
                "__AVR_ATmega328P__",
                "F_CPU=16000000L"
            ],
            "intelliSenseMode": "gcc-x86",
            "compilerPath": "/usr/bin/avr-gcc",
            "cStandard": "c11",
            "cppStandard": "c++11"
        }
    ]
}
